generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id            Int @id @default(autoincrement())
  title         String
  release_date  DateTime

  @@map("books")
}

model Publisher {
  codigo          Int @id @default(autoincrement())
  place_of_origin String
  trading_name    String

  @@map("publishers")
}