generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model Book {
  id            Int        @id @default(autoincrement())
  title         String
  link_img      String
  release_date  DateTime

  @@map("books")
}

model Publisher {
  id                Int     @id @default(autoincrement())
  place_of_origin   String
  trading_name      String
  link_img          String

  @@map("publishers")
}